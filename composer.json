{
	"name": "biwse/biwse-php",
	"description": "The Biwse PHP library provides access to the Biwse API.",
	"version": "1.0.1",
	"keywords": [
		"biwse",
		"bitcoin payments",
		"api",
		"merchant service"
	],
	"homepage": "https://biwse.com/",
	"license": "MIT",
	"authors": [
		{
			"name": "Biwse",
			"email": "support@biwse.com",
			"homepage": "https://github.com/biwse"
		}
	],
	"require": {
		"php": ">=7.1",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"guzzlehttp/guzzle": "^6.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.4",
		"squizlabs/php_codesniffer": "~2.6",
		"friendsofphp/php-cs-fixer": "~2.12"
	},
	"autoload": {
		"psr-4": {
			"Biwse\\": "lib/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Biwse\\": "test/"
		}
	}
}
